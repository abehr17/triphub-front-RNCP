          "WRITE_EXTERNAL_STORAGE",
          "READ_EXTERNAL_STORAGE",
          "READ_MEDIA_STORAGE",
          "MANAGE_DOCUMENTS",
          "ACCESS_FINE_LOCATION",
          "ACCESS_COARSE_LOCATION"
        ]
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "94fa7e30-ce7a-466b-a00f-312028da78d8"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
